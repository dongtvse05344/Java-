<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GetAllFlights</servlet-name>
        <servlet-class>dongtv.controller.GetAllFlights</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetFlyFor</servlet-name>
        <servlet-class>dongtv.controller.SetFlyFor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertFlyController</servlet-name>
        <servlet-class>dongtv.controller.InsertFlyController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllFlights</servlet-name>
        <url-pattern>/GetAllFlights</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetFlyFor</servlet-name>
        <url-pattern>/SetFlyFor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertFlyController</servlet-name>
        <url-pattern>/InsertFlyController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>GetAllFlights</welcome-file>
    </welcome-file-list>
</web-app>
