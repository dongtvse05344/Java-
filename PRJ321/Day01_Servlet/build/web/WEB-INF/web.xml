<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>dongtv.servlet.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>dongtv.view.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>dongtv.controllers.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>dongtv.view.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationListController</servlet-name>
        <servlet-class>dongtv.controllers.RegistrationListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationEdit</servlet-name>
        <servlet-class>dongtv.controllers.RegistrationEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationDelete</servlet-name>
        <servlet-class>dongtv.controllers.RegistrationDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ErrrorServlet</servlet-name>
        <servlet-class>dongtv.view.ErrrorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>dongtv.controllers.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>dongtv.controllers.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchController</servlet-name>
        <servlet-class>dongtv.controllers.searchController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationListController</servlet-name>
        <url-pattern>/RegistrationListController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationEdit</servlet-name>
        <url-pattern>/RegistrationEdit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationDelete</servlet-name>
        <url-pattern>/RegistrationDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ErrrorServlet</servlet-name>
        <url-pattern>/ErrrorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/UserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchController</servlet-name>
        <url-pattern>/searchController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>LoginServlet</welcome-file>
    </welcome-file-list>
</web-app>
